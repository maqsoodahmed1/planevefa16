    <div class="blogs-5" data-background-color="gray">
      <div class="container">
        <div class="row">
          <div id="jumptocards" class="col-md-10 ml-auto mr-auto text-center">
            <h2 class="title">Venues</h2>
            <p>
              According to the National Oceanic and Atmospheric Administration,
              Ted, Scambos, NSIDClead scentist, puts the potentially record low
              maximum sea ice extent tihs year down to low ice extent in the
              Pacific and a late drop in ice extent in the Barents Sea.
            </p>
            <br />

            <div class="row" id="venuescards">
              <% venues.forEach(element => {%>
              <div class="col-md-4">
                <div class="card card-blog">
                  <div class="card-image">
                    <a href="/venues/<%=element._id%>">
                      <img
                        style="cursor: pointer;"
                        src="/<%= element.image %>"
                        class="img rounded"
                      />
                    </a>
                  </div>

                  <div class="card-body">
                    <h6 class="category text-primary">
                      <%= element.venueName %>
                    </h6>
                    <h6 class="category text-primary">
                      <%= element.venueCity %>
                    </h6>

                    <p class="card-description">
                      <%= element.venueDescription %>
                    </p>
                    <div class="card-footer"></div>
                  </div>
                </div>
              </div>

              <% }) %>
            </div>
          </div>
        </div>
      </div>
