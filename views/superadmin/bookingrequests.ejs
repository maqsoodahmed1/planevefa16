<div class="main-content">
  <!-- Navbar -->

  <div class="col-md-8">

        <!-- Booking Requests -->
        <div class="tab-pane" id="tab5">
            <h1>Booking Requests</h1>
            <table>
              <thead>
                <tr>
                  <th>Venue Name</th>
                  <th>View Request</th>
                  <th>Forward Request</th>
                  <th>Reject Request</th>
                </tr>
              </thead>
              <tbody>
  
  
  
  
                <% bookings.forEach(element => {%>
  
  
                <tr>
  
                  <td><%= element.bookedVenue.venueName%></td>
  
                  <td><button class="btn btn-primary" data-toggle="modal"
                      data-target=".bd-example-modal-lg1-<%= element._id%>">
                      View Request
                    </button></td>
                  <div class="modal fade bd-example-modal-lg1-<%= element._id%>" tabindex="-1" role="dialog"
                    aria-labelledby="myLargeModalLabel" aria-hidden="true">
                    <div class="modal-dialog modal-lg">
                      <div class="modal-content">
                        <div class="modal-body">
                          <h3>View Request</h3>
                          <p>Venue Name : <%= element.bookedVenue.venueName%> </p>
                          <p>Venue Type :<%= element.bookedVenue.venueType%> </p>
                          <p>Customer Phone :<%= element.PhoneNumber%> </p>
                          <p>Customer email :<%= element.emailAddress%> </p>
                          <p>Date of Event :<%= element.eventDate%> </p>
                          <p>Date of viewing :<%= element.dateOfViewing%> </p>
  
  
  
                        </div>
                      </div>
                    </div>
                  </div>
          </div>
  
          <td><button type="button" class="btn btn-success" data-toggle="modal" data-target="#forwardRequest">
              Forward Request
            </button></td>
            <!-- Approve Modal Start -->
          <!-- Modal -->
          <div class="modal fade" id="forwardRequest" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
          aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Forward Request</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body">
                <p>Do You Want To Forward This Request <%= element._id%> </p>
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <form action="/superadmin/forwardbooking/<%= element._id %>" method="POST">
                <button type="submit" class="btn btn-primary">Forward</button>
              </form>
              </div>
            </div>
          </div>
        </div>
        <!-- Approve Modal End -->

  
          <td>
            <form action="/superadmin/rejectbooking/<%= element._id %>" method="POST">
  
              <input class="btn btn-warning" type="submit" value="REJECT REQUEST" />
            </form>
  
          </td>
          </tr>
          <% }) %>
  
  
  
          </tbody>
          </table>
          
          <!-- Decline Modal Start -->
  
          <div class="modal fade" id="rejectRequest" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Reject Reason</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                  <textarea name="decline" id="decline" cols="50" rows="8"
                    placeholder="Enter your reason here.."></textarea>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
  
                  <button type="button" class="btn btn-default">Send</button>
                </div>
              </div>
            </div>
          </div>
  
          <!-- Declin Modal Ends -->
  
        </div>
      </div>
    </div>
  
    </div>
    <%- include ("../partials/adminfooter")%>
    <!-- modal end -->  